{
  "expo": {
    "name": "Trackr",
    "slug": "trackr",
    "version": "0.0.8",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "trackr",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "icon": "./assets/app.icon",
      "supportsTablet": true,
      "bundleIdentifier": "com.trackr.client",
      "infoPlist": {
        "CFBundleDisplayName": "Trackr",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ted.trackr.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font",
      "expo-web-browser",
      "expo-secure-store",
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "7571c77a-00f5-4dde-869a-5412c1b46609"
      }
    }
  }
}
